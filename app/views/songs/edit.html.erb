<h2>Edit <%=@song.title  %></h2>

<%= form_for @song do |f| %>
    <%= f.label :title, "Title:" %>
    <%= f.text_field :title %><br>
    <%= f.label :artist_name, "Artist Name:" %>
    <%= f.text_field :artist_name %><br>
    <%= f.label :genre, "Genre:" %>
    <%= f.text_field :genre %><br>
    <%= f.label :release_year, "Release Year:" %>
    <%= f.number_field :release_year %><br>

    <%= f.submit %>
<% end %>